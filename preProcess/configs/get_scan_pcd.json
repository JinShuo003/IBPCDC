{
  "mesh_dir": "E:\\dataset\\IBPCDC\\mesh",
  "pcd_partial_save_dir": "E:\\dataset\\IBPCDC\\pcdScan\\IBSNet_scan_1024",

  "category_re": "scene\\d",
  "scene_re": "scene\\d.\\d{4}",
  "filename_re": "scene\\d.\\d{4}",

  "handle_category": "scene9",
  "handle_scene": "scene\\d.\\d{4}",
  "handle_filename": "scene\\d.\\d{4}",

  "save_data": true,
  "visualize": true,
  "process_num": 1,

  "scan_options": {
