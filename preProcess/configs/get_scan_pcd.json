    "file_log_options": {
      "file_path": "get_scan_pcd.log",
      "log_level": "DEBUG",
      "is_need_formatter": false,
      "formatter": "%(asctime)s - %(filename)s[line:%(lineno)d] - %(levelname)s: %(message)s"
    },
      "console_log_options": {
      "log_level": "INFO",
      "is_need_formatter": false,
      "formatter": "%(asctime)s - %(filename)s[line:%(lineno)d] - %(levelname)s: %(message)s"
    }
  }
}
